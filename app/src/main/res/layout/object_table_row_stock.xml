    android:layout_width="wrap_content"
    android:layout_height="wrap_content"
    android:orientation="horizontal">

    <TextView
        android:id="@+id/stock_name"
        android:textSize="20sp" />

    <TextView
        android:id="@+id/stock_count"
        android:textSize="20sp" />

    <TextView
        android:id="@+id/stock_avg_price"
        android:textSize="20sp" />

</TableRow>
